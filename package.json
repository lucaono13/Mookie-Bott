{
    "name": "mookie-bot",
    "version": "0.0.1",
    "author": "David Taylor, Gianluca Onorato, Erkin George",
    "description": "A discord.js bot written with TypeScript",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=16.9.0"
    },
    "type": "module",
    "exports": [
        "./dist/start-bot.js",
        "./dist/start-manager.js"
    ],
    "scripts": {
        "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "clean": "git clean -xdf --exclude=\"/config/**/*\"",
        "clean:dry": "git clean -xdf --exclude=\"/config/**/*\" --dry-run",
        "build": "tsc --project tsconfig.json",
        "commands:view": "yarn run build && node --enable-source-maps dist/start-bot.js commands view",
        "commands:register": "yarn run build && node --enable-source-maps dist/start-bot.js commands register",
        "commands:rename": "yarn run build && node --enable-source-maps dist/start-bot.js commands rename",
        "commands:delete": "yarn run build && node --enable-source-maps dist/start-bot.js commands delete",
        "commands:clear": "yarn run build && node --enable-source-maps dist/start-bot.js commands clear",
        "start": "yarn run start:bot",
        "start:bot": "yarn run build && node --enable-source-maps dist/start-bot.js",
        "start:manager": "yarn run build && node --enable-source-maps dist/start-manager.js",
        "start:pm2": "yarn run build && yarn run pm2:start",
        "pm2:start": "pm2 start process.json",
        "pm2:stop": "pm2 stop process.json",
        "pm2:delete": "pm2 delete process.json"
    },
    "dependencies": {
        "@discordjs/rest": "1.7.1",
        "Math": "^0.0.1-security",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "discord.js": "14.11.0",
        "discord.js-rate-limiter": "1.3.2",
        "dotenv": "^16.3.1",
        "express": "4.18.2",
        "express-promise-router": "4.1.1",
        "filesize": "10.0.7",
        "linguini": "1.3.1",
        "luxon": "3.3.0",
        "math": "^0.0.3",
        "mongodb": "^6.6.1",
        "node-fetch": "3.3.1",
        "node-schedule": "2.1.1",
        "pino": "8.14.1",
        "pino-pretty": "10.0.0",
        "pm2": "^5.3.0",
        "reflect-metadata": "^0.1.13",
        "remove-markdown": "0.5.0",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@types/express": "4.17.17",
        "@types/luxon": "3.3.0",
        "@types/node": "^20.3.3",
        "@types/node-schedule": "2.1.0",
        "@types/remove-markdown": "0.3.1",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^5.59.6",
        "@typescript-eslint/parser": "^5.59.6",
        "eslint": "^8.41.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-unicorn": "^47.0.0",
        "prettier": "^2.8.8",
        "typescript": "^5.0.4"
    },
    "packageManager": "yarn@3.6.0"
}
